                <a href = "/about">о проекте</a>
                <a href = "/alexandra">Элбакян</a>
                <a href = "/stats">статистика</a>
                <a href = "/donate" class = "donate">помочь</a>
            </div>
            
            <div id = "back"></div>
            <div id = "bulb"><img src = "/pictures/bulb.png"></div>

        </div><div id = "info">

            <div id = "lang">
                <span class = "he">язык</span>
                <span class = "list"><a href = # class = "selected">Русский</a><a href = # onclick = "language('en')">English</a><a href = # onclick = "language('cn')">简体中文</a><a href = # onclick = "language('pt')">Português</a></span>
